{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "YWRtaW4=",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "events.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "events.dlx",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "pedido.projection.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "events.dlx",
        "x-dead-letter-routing-key": "pedido.projection.dlq",
        "x-message-ttl": 300000
      }
    },
    {
      "name": "pedido.projection.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "events.exchange",
      "vhost": "/",
      "destination": "pedido.projection.queue",
      "destination_type": "queue",
      "routing_key": "pedido.*",
      "arguments": {}
    },
    {
      "source": "events.dlx",
      "vhost": "/",
      "destination": "pedido.projection.dlq",
      "destination_type": "queue",
      "routing_key": "pedido.projection.dlq",
      "arguments": {}
    }
  ]
